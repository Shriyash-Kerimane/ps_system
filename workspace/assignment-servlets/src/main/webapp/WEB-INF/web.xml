<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	id="WebApp_ID" version="3.1">
	<display-name>servlet-app</display-name>

	<servlet>
		<servlet-name>welcome</servlet-name>
		<servlet-class>com.demo.web.Question1</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>greet</servlet-name>
		<servlet-class>com.demo.web.Question2</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>userDetail</servlet-name>
		<servlet-class>com.demo.web.NameEmail</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>welcome</servlet-name>
		<url-pattern>/question1</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>greet</servlet-name>
		<url-pattern>/question2</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>userDetail</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	


</web-app>